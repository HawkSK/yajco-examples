BEGINNING-OF-PROGRAM
	DEFINE TURNRIGHT AS
		ITERATE 3 TIMES
			TURNLEFT
	BEGINNING-OF-EXECUTION
		TURNLEFT
		ITERATE 2 TIMES
			MOVE
		TURNRIGHT
		ITERATE 2 TIMES
			MOVE
		PICKBEEPER
		WHILE NOT-FACING-EAST DO
			TURNLEFT
		MOVE
		IF ANY-BEEPERS-IN-BEEPER-BAG THEN
			PUTBEEPER
		ELSE
			MOVE
		ENDIF
		ITERATE 2 TIMES
			MOVE
		IF FACING-EAST THEN
			BEGIN
			    TURNRIGHT
				MOVE
				PICKBEEPER
				MOVE
				WHILE NOT-FACING-WEST DO
				    TURNLEFT
				    MOVE
			END
		ELSE
			MOVE
		ENDIF
		TURNOFF
	END-OF-EXECUTION
END-OF-PROGRAM
